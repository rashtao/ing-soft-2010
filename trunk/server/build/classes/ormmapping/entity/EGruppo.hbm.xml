<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.EGruppo" table="EGruppo" lazy="false">
		<id name="idGruppo" column="IdGruppo" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="nome" column="Nome" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_eContatto" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="EGruppoIdGruppo" not-null="false"/>
			<one-to-many class="entity.EContatto"/>
		</set>
	</class>
</hibernate-mapping>
